{
    "env": {
        "node": true
    },
    "extends": "airbnb-base",
    "rules": {
      "quotes": ["error", "single", {"allowTemplateLiterals": true}],
      "no-plusplus": 0,
      "class-methods-use-this": false
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    }
}